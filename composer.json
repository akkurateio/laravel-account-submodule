{
    "name": "akkurateio/laravel-account-submodule",
    "description": "Manage your users and accounts inside this package",
    "type": "library",
    "authors": [
        {
            "name": "Akkurate",
            "email": "hello@subvitamine.com",
            "homepage": "https://www.akkurate.io",
            "role": "Company"
        }
    ],
    "support": {
        "docs": "https://github.com/akkurateio/documentation"
    },
    "require": {
        "cviebrock/eloquent-sluggable": "^8.0",
        "spatie/laravel-permission": "^3.0",
        "webpatser/laravel-uuid": "^4.0",
        "laravel/passport": "^10.0",
        "spatie/laravel-searchable": "^1.9",
        "laravel/ui": "^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0"
    },
    "autoload" : {
        "psr-4": {
            "Akkurate\\LaravelAccountSubmodule\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "stubs/app",
            "Database\\Seeders\\": "database/seeders",
            "Database\\Factories\\": "database/factories"
        }
    },
    "extra": {
    "laravel": {
        "providers": [
            "Akkurate\\LaravelAccountSubmodule\\LaravelAccountSubmoduleServiceProvider"
        ]
    }
}
}
